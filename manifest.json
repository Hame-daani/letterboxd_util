{
    "manifest_version": 3,
    "name": "30Nama",
    "description": "A simple content_scripts example.",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": [
                "https://letterboxd.com/film/*"
            ],
            "js": [
                "script.js"
            ],
            "run_at": "document_end"
        }
    ],
    "permissions": [
        "activeTab"
    ],
    "action": {
        "default_popup": "popup.html"
    },
    "icons": {
        "16": "icon.png",
        "32": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    }
}